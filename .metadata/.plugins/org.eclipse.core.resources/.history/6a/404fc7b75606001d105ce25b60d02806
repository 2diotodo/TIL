package java04.control;

public class IF03 {
	public static void main(String[] args) {
		//if(조건식) {} else {} 조건식이 참일 경우 if블록 내부 수행/ 거짓이면 else 블록 수행
		
		int score = 87;
		
		//90점 이상
		if(score <90) {
			System.out.println("A");
		}else {
			System.out.println("맥주를 마실 수 있습니다.");
		}
		
		int num;
		if(age < 20) {
			num =10;
		}else {
			num = 20;
		}
		
		System.out.println(num);
	}
}
