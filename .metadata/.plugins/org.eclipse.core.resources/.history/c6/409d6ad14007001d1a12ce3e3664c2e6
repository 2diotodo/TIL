package com.ssafy.ws.step3;

import java.util.Scanner;

public class DigitTest1 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt();
		int line = 1;
		
		for(int i=1;line>=n ;i++) {
			line += i;
		}
		
		for(int i=0; i<n; i++) {
			
			System.out.print(i+" ");
		}
	}//end main()
}//end class
